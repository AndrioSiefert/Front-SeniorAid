{
  "plugins": ["prettier-plugin-tailwindcss"],
  "printWidth": 120,
  "htmlWhitespaceSensitivity": "ignore",
  "jsxWrapAttributes": "preserve",
  "bracketSameLine": false,
  "singleQuote": true,
  "trailingComma": "all",
  "arrowParens": "always",
  "tabWidth": 4,
  "semi": true,
  "useTabs": false,
  "endOfLine": "auto",
  "jsxSingleQuote": true,
  "jsxBracketSameLine": false,
  "bracketSpacing": true,
  "insertPragma": false,
  "requirePragma": false,
  "proseWrap": "preserve"
}
